{
  "name": "@intake24-dietician/api-new",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "src/app.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.build.json",
    "prebuild": "pnpm --filter @intake24-dietician/common build && pnpm --filter @intake24-dietician/db-new build",
    "build": "run-s prebuild clean compile",
    "dev": "nodemon",
    "start": "DOTENV_CONFIG_PATH=.env.staging node -r dotenv/config ./dist/server.js",
    "lint": "eslint . --ext .js,.ts --fix",
    "test:build": "pnpm build:clean && webpack --env NODE_ENV=test",
    "test:unit": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test jest --logHeapUsage",
    "test:unit:coverage": "pnpm test:unit --coverage",
    "test:unit:watch": "pnpm test:unit:coverage --watch",
    "test:integration": "jest ./__tests__/integration/index.test.ts --runInBand --testMatch **/__tests__/integration/**/*.test.ts",
    "test:integration:coverage": "pnpm test:integration --coverage",
    "test:integration:watch": "pnpm test:integration --watch",
    "barrels": "barrelsby --config .barrelsby.json",
    "cli:user-management": "DOTENV_CONFIG_PATH=.env.development ts-node -r dotenv/config -r tsconfig-paths/register src/cli/user-management.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
