{
  "name": "@intake24-dietician/auth",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "src/app.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsoa spec-and-routes && tsc --outDir build --experimentalDecorators",
    "build:clean": "rimraf build",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\" --env NODE_ENV=development",
    "start": "node build/apps/auth/src/server.js",
    "lint": "eslint . --ext .js,.ts --fix",
    "test:build": "pnpm build:clean && webpack --env NODE_ENV=test",
    "test:unit": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test jest --logHeapUsage",
    "test:unit:coverage": "pnpm test:unit --coverage",
    "test:unit:watch": "pnpm test:unit --watch",
    "test:integration": "jest ./__tests__/integration/index.test.ts --runInBand --testMatch **/__tests__/integration/**/*.test.ts",
    "test:integration:coverage": "pnpm test:integration --coverage",
    "test:integration:watch": "pnpm test:integration --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@t3-oss/env-core": "^0.6.1",
    "@tsoa/runtime": "^5.0.0",
    "argon2": "^0.31.1",
    "awilix": "^8.0.1",
    "chai-http": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mailtrap": "^3.2.0",
    "minimatch": "^9.0.3",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0",
    "sequelize-mock": "^0.10.2",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/body-parser": "^1.19.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.7.0",
    "@types/nodemailer": "^6.4.11",
    "@types/supertest": "^2.0.13",
    "@types/swagger-ui-express": "^4.1.4",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pino-pretty": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsconfig-paths": "^4.2.0",
    "why-is-node-running": "^2.2.2"
  }
