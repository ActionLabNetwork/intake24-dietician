    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "1.4.5-lts.1",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
