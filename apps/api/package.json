    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.7",
    "@types/supertest": "^2.0.13",
    "@types/swagger-ui-express": "^4.1.4",
    "concurrently": "^8.2.1",
    "dotenv-webpack": "^8.0.1",
